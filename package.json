{
  "name": "@andyluo/utils",
  "type": "module",
  "version": "0.0.6",
  "packageManager": "pnpm@9.15.5",
  "description": "collection of common JavaScript / TypeScript utils by @andyLuo",
  "author": "@andyLuo",
  "license": "MIT",
  "homepage": "https://github.com/andylou0102/utils#readme",
  "repository": {
    "type": "git",
    "url": "git+https://git@github.com/andylou0102/utils.git"
  },
  "bugs": {
    "url": "https://github.com/andylou0102/utils/issues"
  },
  "keywords": [
    "utils"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs"
    }
  },
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "bumpp && pnpm publish",
    "start": "esno src/index.ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "devDependencies": {
    "@andyluo/eslint-config": "^2.3.0",
    "@types/node": "^18.19.76",
    "@types/throttle-debounce": "^5.0.2",
    "bumpp": "^9.11.1",
    "eslint": "^9.21.0",
    "esno": "^0.16.3",
    "throttle-debounce": "5.0.0",
    "typescript": "^4.9.5",
    "unbuild": "^3.3.1",
    "vite": "^3.2.11",
    "vitest": "^0.25.8"
  }
}
